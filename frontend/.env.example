# Environment Variables for Twitter Handler Frontend

# API Configuration
REACT_APP_API_URL=http://localhost:8000
REACT_APP_ENVIRONMENT=development

# Feature Flags
REACT_APP_ENABLE_DARK_MODE=true
REACT_APP_ENABLE_DIRECT_POSTING=false
REACT_APP_ENABLE_ANALYTICS=true

# Twitter Configuration (if needed for direct posting)
# REACT_APP_TWITTER_API_KEY=your_api_key_here
# REACT_APP_TWITTER_API_SECRET=your_api_secret_here

# Development Settings
REACT_APP_DEBUG=true
REACT_APP_LOG_LEVEL=debug
